<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>sss.servs</groupId>
    <artifactId>tag</artifactId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <groupId>sss.servs</groupId>
  <artifactId>tag.impl</artifactId>
  <version>1.0-SNAPSHOT</version>
  <name>tag.impl</name>
  <url>http://maven.apache.org</url>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  
  <dependencies>
    
    <dependency>
      <groupId>sss.servs</groupId>
      <artifactId>circle.serv</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>sss.servs</groupId>
      <artifactId>entity.datatypes</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>sss</groupId>
      <artifactId>conf.conf</artifactId>
      <version>${project.version}</version>
    </dependency>
   
    <dependency>
      <groupId>sss.serv</groupId>
      <artifactId>serv.custom.caller</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>sss.servs</groupId>
      <artifactId>ue.datatypes</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>sss.servs</groupId>
      <artifactId>tag.datatypes</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>sss.servs</groupId>
      <artifactId>tag.api</artifactId>
      <version>${project.version}</version>
    </dependency>
    
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>
